<Project>
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net471</TargetFrameworks>
        <MRTKRoot>..\..</MRTKRoot>
        <UnityInstallRoot>C:\Program Files\Unity</UnityInstallRoot>
        <UnityManagedRoot>$(UnityInstallRoot)\Editor\Data\Managed</UnityManagedRoot>
        <UnityEngineDll>$(UnityManagedRoot)\UnityEngine.dll</UnityEngineDll>
        <UnityEngineUIExtensionDll>$(UnityInstallRoot)\Editor\Data\UnityExtensions\Unity\GUISystem\UnityEngine.UI.dll</UnityEngineUIExtensionDll>
        <OutputPath>$(MRTKRoot)\Build\__output</OutputPath>
        <MRTKConstants>UNITY_2018_1_OR_NEWER</MRTKConstants>
        <DefineConstants>$(MRTKConstants)</DefineConstants>
        
        <Version>1.0.0-alpha01</Version>
        <Authors>Microsoft</Authors>
        <PackageLicenseUrl>https://github.com/Microsoft/MixedRealityToolkit-Unity/blob/master/License.txt</PackageLicenseUrl>
        <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
        <ContentTargetFolders>MixedRealityToolkit</ContentTargetFolders>
    </PropertyGroup>
</Project>